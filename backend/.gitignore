# Ignore node_modules directory
node_modules/

# Ignore log files
*.log

# Ignore compiled binaries
*.exe
*.dll
*.so
*.dylib

# Ignore macOS system files
.DS_Store

# Ignore environment-specific files
.env

# Ignore npm debug logs
npm-debug.log

